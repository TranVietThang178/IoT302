 */
#include <stdio.h>

int main(void) {
	for (int i = 0; i < 9; i++){
		if (i < 5) {
			for (int j = 0; j < 5 - i; j++) {
				printf(" ");
			}
			for (int k = 0; k < 2 * i - 1; k++) {
				printf("*");
			}
		} else {
			for (int j = 0; j < 0; j++) {
				printf(" ");
			}
			for (int k = 0; k < 0; k++) {
				printf("*");
			}
		}
		printf("\n");
	}
}

