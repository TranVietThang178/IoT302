#include <stdio.h>

int main(void)
{
	int a, b, c;

	printf("a = ");
	scanf("%d", &a);
	printf("b = ");
	scanf("%d", &b);
	printf("c = ");
	scanf("%d", &c);

	int A = a++ + ++a;
	int B = --a - b-- * ++c;

	printf("A = %d", (a++) + (++a));
	printf("B = %d", --a - b-- * ++c);

	getchar();
	getchar();

	return 0;
}
