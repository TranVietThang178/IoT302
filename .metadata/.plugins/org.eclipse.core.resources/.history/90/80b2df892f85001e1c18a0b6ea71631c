#include <stdio.h>

int main(void){
	int temperature, humidity;

	printf("Temp: ");
	scanf("%d", &temperature);
	printf("Humid: ");
	scanf("%d", &humidity);

	if (temperature < 30) {
		printf("Cooling mode ON");
	} else
	{
		printf("Cooling mode OFF");
	}

	if (humidity > 70) {
		printf("Fan mode ON");
	} else
	{
		printf("Fan mode OFF");
	}

	getchar();
	getchar();

	return 0;
}
